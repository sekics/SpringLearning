<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.portrait.dao.StandardMapper">

    <insert id="insertStandadMap" parameterType="map">
        insert into portrait.standard (author_id,author_name,time,name,level,total_person,identity_rank)
        values(#{authorId},#{authorName},#{time},#{name},#{level},#{totalPerson},#{identityRank})
    </insert>

</mapper>